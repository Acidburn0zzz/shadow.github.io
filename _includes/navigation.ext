<div class="nav">
  <ul>
    {% assign url = page.url|remove:'index.html' %}
    {% for link in page.navigation limit: 5 %}
    <li {% if url == link.url %}class="navactive"{% endif %}>
        <a href="{{link.url}}" title="{{link.title}}">{{link.text}}</a>
    </li>
    {% endfor %}
  </ul>
</div>
