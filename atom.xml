---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>shadow.github.io</title>
  <link href="http://shadow.github.io/"/>
  <link type="application/atom+xml" rel="self" href="http://shadow.github.io/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://shadow.github.io/</id>
  <author>
    <name>Shadow</name>
    <email>shadow--support@cs.umn.edu</email>
  </author>
 
  {% for post in site.posts %}
  <entry>
    <id>http://shadow.github.io{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://shadow.github.io{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Shadow</name>
      <uri>http://shadow.github.io/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
